  <script>
    function capturar()
    {
        
        // Obtenemos el valor por el id
        var porId=document.getElementById("seleccion").value;
        
       
			if (porId==2){
   			
			plotTimes1();
			}
			if (porId==1){
   			
			plotTimes();
			}
			if (porId==3){
   			
			plotTimes2();
			}
			
			if (porId==4){
   			
			plotTimes3();
			}
			if (porId==5){
   			
			plotTimes4();
			}
    }
    </script>

